    "\n",
    "dev = qml.device(\"alicebob.simulator\")\n",
    "\n",
    "@qml.qnode(dev)\n",
    "def circuit():\n",
    "    qml.Hadamard(wires=0)\n",
    "    return qml.state()\n",
    "\n",
    "print(circuit())"
